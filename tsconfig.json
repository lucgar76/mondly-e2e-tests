{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "types": ["cypress", "node"],
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["cypress/**/*.ts", "cypress.config.ts"]
  }